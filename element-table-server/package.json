  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.5.0",
    "tsoa": "^4.1.3"
  },
  "nodemonConfig": {
    "watch": [
      "src"
