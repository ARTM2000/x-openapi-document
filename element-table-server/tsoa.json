{
    "entryFile": "src/main.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "spec": {
        "outputDirectory": "public",
        "specVersion": 3,
        "securityDefinitions": {
            "api_key": {
                "type": "apiKey",
                "name": "authorization",
                "in": "header"
            }
        }
    },
    "routes": {
        "authenticationModule": "./auth-middleware.ts"
    }
}